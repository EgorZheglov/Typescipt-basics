{
  "name": "y",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "commonjs",
  "description": "REST service starter for RS School",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "rimraf dist && tsc",
    "lint": "eslint --ext .js,.ts --fix src/",
    "migrate": "typeorm migrations:generate -n"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.0",
    "await-to-js": "^3.0.0",
    "bcrypt": "^5.0.1",
    "cross-env": "7.0.3",
    "dotenv": "^10.0.0",
    "express": "4.17.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "4.3.0",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.3",
    "@types/yamljs": "^0.2.31",
    "chai": "4.3.4",
    "debug": "4.3.3",
    "dirty-chai": "2.0.1",
    "eslint": "8.5.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "husky": "^7.0.4",
    "jest": "27.4.5",
    "lint-staged": "12.1.3",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "supertest": "6.1.6",
    "ts-node": "^10.4.0",
    "tslint": "^5.20.1",
    "typescript": "^4.5"
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "author": "Egor Zheglov"
